{{#each meals as |meal|}}
<div class="meal-card">
  <a href="/meals/{{meal.id}}">
  <div class="meal-card-header">
    {{meal.name}}
  </div>
  </a>
  <div class="card-body">
    <p>
      Instructions:
      {{meal.instructions}}
    </p>
    <p>
      Meal created by:
      {{meal.user_id}}
    </p>
  </div>
</div>
{{/each}}